{
  "name": "ts-coinmarketcap",
  "version": "1.0.0",
  "description": "A modern, lightweight, and fully-typed TypeScript client for the CoinMarketCap API.",
  "keywords": [
    "coinmarketcap",
    "api",
    "client",
    "crypto",
    "cryptocurrency",
    "typescript",
    "wrapper",
    "btc",
    "eth",
    "bitcoin",
    "ethereum",
    "blockchain",
    "market-data"
  ],
  "homepage": "https://github.com/shahradelahi/ts-coinmarketcap#readme",
  "repository": "github:shahradelahi/ts-coinmarketcap",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./types/*": "./dist/types/*/index.d.ts"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "!types/**/*.js",
    "!**/*.d.cts"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "git clean -fdx node_modules dist .tsbuildinfo",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm build && pnpm test && pnpm lint && pnpm format:check",
    "test": "mocha",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "dependencies": {
    "@se-oss/rand": "^1.1.0",
    "@xmldom/xmldom": "^0.9.8",
    "deepmerge": "^4.3.1",
    "ky": "^1.8.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@shahrad/eslint-config": "^1.0.0",
    "@shahrad/prettier-config": "^1.2.2",
    "@shahrad/tsconfig": "^1.1.0",
    "@types/chai": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.16.5",
    "chai": "^5.2.1",
    "dotenv": "^16.6.1",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "mocha": "^11.7.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
